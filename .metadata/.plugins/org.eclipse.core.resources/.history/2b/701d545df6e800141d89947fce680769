
public class prueba {

	public static void main(String[] args) {
		Gestor g = new Gestor();
		proyecto p1 = new proyecto(g.getId_proyectos());
		g.agregarProyecto(p1);
		tarea t1 = new tarea(g.getId_tareas());
		g.agregarTarea(t1, p1.getId());
		for (int i = 0; i < g.getContador_proyectos(); i++) {
			proyecto p = g.getProyectos().get(i);
			System.out.println(p.getNombre());
			for (int j = 0; j < g.getContador_tareas(); j++) {
				g.exportarTarea(p.getTareas().get(j).getId(),"prueba1.bin");
				
			}
		}
		System.out.println("hola");
	}

}
